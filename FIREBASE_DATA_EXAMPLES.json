      },
      "edu2": {
        "id": "edu2",
        "title": "Lung Cancer",
        "short": "Leading cause of cancer deaths",
        "details": "Symptoms: Persistent cough, chest pain, shortness of breath, coughing up blood, weight loss. Treatment: Surgery, chemotherapy, radiation, targeted therapy, immunotherapy. Prevention: Avoid smoking, limit exposure to radon and asbestos, healthy diet.",
        "moreLink": "https://www.who.int/health-topics/cancer"
      },
      "edu3": {
        "id": "edu3",
        "title": "Colon Cancer",
        "short": "Third most common cancer type",
        "details": "Symptoms: Changes in bowel habits, blood in stool, abdominal pain, unexplained weight loss, fatigue. Treatment: Surgery, chemotherapy, radiation therapy. Prevention: Regular screenings (colonoscopy), high-fiber diet, limit red meat, exercise regularly.",
        "moreLink": "https://www.who.int/health-topics/cancer"
      }
